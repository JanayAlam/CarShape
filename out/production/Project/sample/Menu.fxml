<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="900.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MenuController">
   <children>
      <Button fx:id="buttonNewGame" layoutX="71.0" layoutY="389.0" mnemonicParsing="false" onAction="#setNewGame" prefHeight="45.0" prefWidth="250.0" text="New Game">
         <font>
            <Font size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="labelControlKeys" layoutX="71.0" layoutY="444.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="250.0" text="Control Keys">
         <font>
            <Font size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="buttonCredits" layoutX="71.0" layoutY="499.0" mnemonicParsing="false" onAction="#setCredits" prefHeight="45.0" prefWidth="250.0" text="Credits">
         <font>
            <Font size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="buttonExit" layoutX="71.0" layoutY="554.0" mnemonicParsing="false" onAction="#setButtonExit" prefHeight="45.0" prefWidth="250.0" text="EXIT">
         <font>
            <Font size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label layoutX="71.0" layoutY="302.0" text="SHAPEcars">
         <font>
            <Font name="System Bold" size="40.0" />
         </font>
      </Label>
      <Label fx:id="labelWelcome" layoutX="71.0" layoutY="369.0" text="Welcome, Alam" textAlignment="RIGHT" />
   </children>
</AnchorPane>
